<h1><%= page.title %></h1>
<h4 class="subtitle h5 m-y-2"><%= page.subtitle %></h4>

<ul class="list-inline">
    <li class="list-inline-item"><%- link_to(page.repo, 'Source', {class: "btn btn-primary btn-sm", external: true}) %></li>
    <%- _.map(page.links, (url, text) => `<li class="list-inline-item">${link_to(url, text, {class: "btn btn-primary btn-outline-primary btn-sm", external: true})}</li>` )
        .join("\n") %>
</ul>

<hr>

<%- page.content %>
