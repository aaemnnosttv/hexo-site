<% var posts = page.posts %>
<% if (is_home()) { %>
    <% posts = page.posts.skip(1); %>
    <%- partial('./partial/home-box', {post: page.posts.first()}) %>
<% } %>

<% posts.each(function(post) { %>
    <%- partial('./partial/article-row-item', {post: post}) %>
<% }) %>

<%- partial('./widget/paginator') %>
